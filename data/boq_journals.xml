    <!-- BOQ Retention Journal -->
    <record id="boq_retention_journal" model="account.journal">
        <field name="name">BOQ Retention</field>
        <field name="code">BQRET</field>
        <field name="type">general</field>
        <field name="update_posted">True</field>
    </record>
</odoo>